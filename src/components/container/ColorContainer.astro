---
import Card from '../Card.astro'
const {colorImage, name} = Astro.props;
---

<Card>
  <div class="flex justify-self-center justify-between items-center gap-5 sm:w-[150px]">
    <svg class="w-10 h-10 m-2">
      <circle cx="5" cy="5" r="4" fill={name} stroke="#373A50" stroke-width="2"/>
    </svg>
    <p>
      {name}
    </p>
  </div>
</Card>